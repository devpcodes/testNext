{
    "name": "new-newweb",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "node server.js",
        "build": "next build",
        "start": "cross-env NODE_ENV=production node server.js",
        "pm2": "cross-env NODE_ENV=production pm2 start server.js -n new-newweb",
        "pm2:dev": "cross-env NODE_ENV=development pm2 start server.js -n new-newweb",
        "lint": "eslint --ignore-path .gitignore .",
        "prettier": "prettier \"**/*.+(js|jsx|json|css|html|md)\"",
        "format": "yarn prettier --write",
        "check-format": "yarn prettier --list-different",
        "validate": "yarn check-format && yarn lint"
    },
    "dependencies": {
        "@amcharts/amcharts4": "^4.9.34",
        "antd": "^4.5.2",
        "axios": "^0.20.0",
        "cross-env": "^7.0.2",
        "express": "^4.17.1",
        "image-trace-loader": "^1.0.2",
        "imagemin-gifsicle": "^7.0.0",
        "imagemin-mozjpeg": "^9.0.0",
        "imagemin-optipng": "^8.0.0",
        "imagemin-svgo": "^8.0.0",
        "jimp": "^0.16.1",
        "jwt-decode": "^3.0.0-beta.2",
        "lqip-loader": "^2.2.1",
        "moment": "^2.29.0",
        "next": "9.5.1",
        "next-optimized-images": "^2.6.2",
        "next-pwa": "^5.0.0",
        "next-redux-wrapper": "^6.0.2",
